# Environment variables for Deep Research Agent
# Copy this file to .env and fill in your values

# Required: Google API key for Gemini LLM
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Override default configurations
# LLM_MODEL=gemini-2.5-flash
# LLM_TYPE=gemini
# EMBED_MODEL=BAAI/bge-small-en-v1.5
# RERANKER_MODEL=colbert-ir/colbertv2.0

# Optional: Directory paths (useful for production deployment)
# PDF_DIRECTORY=./data
# STORAGE_DIR=./storage
# RESEARCH_OUTPUT_DIR=./research_outputs

# Optional: Research configuration
# MAX_REASONING_STEPS=10
# ENABLE_QUERY_DECOMPOSITION=True
# ENABLE_MULTI_SOURCE_SYNTHESIS=True

# Optional: Retrieval configuration
# VECTOR_TOP_K=10
# BM25_TOP_K=10
# RERANKER_TOP_N=5

# Production settings (automatically set by Render)
# PORT=10000
# RENDER_ENV=production
# PYTHONUNBUFFERED=1